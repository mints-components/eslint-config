{
  "name": "@mints/eslint-config",
  "packageManager": "yarn@4.7.0",
  "version": "2.2.0",
  "description": "Eslint Config for Mints Components",
  "keywords": [
    "mints",
    "mints-components",
    "eslint",
    "config"
  ],
  "type": "module",
  "files": [
    "index.js"
  ],
  "license": "MIT",
  "homepage": "https://github.com/mints-components/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mints-components/eslint-config.git"
  },
  "main": "index.js",
  "scripts": {
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier . --write"
    ]
  },
  "dependencies": {
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "typescript-eslint": "^8.26.1"
  },
  "peerDependencies": {
    "eslint": "^9",
    "prettier": "^3"
  },
  "devDependencies": {
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "pinst": "^3.0.0",
    "prettier": "^3.0.0"
  }
}
